using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("Ingrese un n√∫mero:");
        int num = Convert.ToInt32(Console.ReadLine());

        long factorial = 1;
        for (int i = 1; i <= num; i++)
        {
            factorial *= i;
        }

        Console.WriteLine("El factorial de " + num + " es: " + factorial);
    }
}
