using System;

class Program
{
    static void Main()
    {
        int[] vec1 = { 1, 2, 3 };
        int[] vec2 = { 4, 5, 6 };

        if (vec1.Length != vec2.Length)
        {
            Console.WriteLine("Los vectores no tienen la misma longitud.");
            return;
        }

        int productoEscalar = 0;
        for (int i = 0; i < vec1.Length; i++)
        {
            productoEscalar += vec1[i] * vec2[i];
        }

        Console.WriteLine("El producto escalar de los vectores es: " + productoEscalar);
    }
}
