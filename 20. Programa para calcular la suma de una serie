using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("Ingrese el valor de n:");
        int n = Convert.ToInt32(Console.ReadLine());
        double suma = 0;

        for (int i = 1; i <= n; i++)
        {
            suma += 1.0 / i;
        }

        Console.WriteLine("La suma de la serie es: " + suma);
    }
}
