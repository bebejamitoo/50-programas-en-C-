using System;

class Program
{
    static void Main()
    {
        Console.WriteLine("Ingrese una cadena:");
        string str = Console.ReadLine();
        string rev = new string(str.Reverse().ToArray());

        if (str == rev)
        {
            Console.WriteLine("La cadena es un palíndromo.");
        }
        else
        {
            Console.WriteLine("La cadena no es un palíndromo.");
        }
    }
}
