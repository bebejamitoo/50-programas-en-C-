using System;
using System.Linq;

class Program
{
    static void Main()
    {
        int[] arr = { 5, 3, 8, 4, 2, 3, 8 };

        arr = arr.Distinct().ToArray();

        Console.WriteLine("Array despu√©s de eliminar duplicados:");
        foreach (int val in arr)
        {
            Console.Write(val + " ");
        }
    }
}
